    },
    {
        "id": "ae098a6105fabf66",
        "type": "function",
        "z": "5f8dd6449c27116e",
        "name": "return formatted articles",
        "func": "let articles = msg.payload.articles;\n\nif (!articles || articles.length === 0) {\n    msg.payload = \"No recent news found for Dmitrijs Meiksans.\";\n} else {\n    let formattedArticles = articles.slice(0, 3).map(article => `${article.title} - ${article.source.name}`).join(\"\n\n\");\n    msg.payload = formattedArticles;\n}\n\nreturn msg;\n\n",
        "outputs": 1,
        "timeout": 0,
        "noerr": 0,
