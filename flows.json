        "z": "5f8dd6449c27116e",
        "name": "Request search from the News API",
        "method": "GET",
        "ret": "obj",
        "paytoqs": "ignore",
        "url": "{{msg.url}}",
        "tls": "",
        "persist": false,
        "proxy": "",
        "insecureHTTPParser": false,
        "authType": "",
        "senderr": false,
